a=c(1,1500,3,10,1,1800,4,8,1,1200,2,15,1,2000,3,5)
A=matrix(a,nrow=4,ncol=4,byrow=T)
A
Y=c(250000,320000,180000,350000)
det(A)

B=t(A)

C=B%*%A
C

I=solve(C)

bh=I%%B%%Y
bh

X1=c(1500,1800,1200,2000)
X2=c(3,4,2,3)
X3=c(10,8,15,5)
Yp=(-8.000000e+04)+((2.000000e+02)*X1)+((1.000000e+04)*X2)+((0)*X3)
Yp

Yb=mean(Y)
col1=Yp-Yb
col1
col1^2
sum1=sum(col1^2)
col2=Y-Yb
col2
col2^2
sum2=sum(col2^2)
Rsq=sum1/sum2; Rsq
