x=1:20
m=c(3.63,4.2,3.33,4.54,2.89,4.87,4.9,5.29,6.18,7.2,7.25,6.09,6.8,8.65,8.43,8.29,7.18,7.9,8.45,8.23)
p=c(0.97,0.95,0.99,0.91,0.98,0.9,0.89,0.86,0.85,0.82,0.79,0.83,0.81,0.77,0.76,0.8,0.83,0.79,0.76,0.78)
model=lm(m~p);model
predicted=predict(model)
r=residuals(model)
plot(predicted,r,ylab="Residuals")
cor.test(r,p,method="spearman")
